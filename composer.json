{
  "name": "cubex/dist",
  "license": "MIT",
  "type": "project",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "require": {
    "cubex/framework": "^4.0",
    "cubex/mv": "^0.2",
    "filp/whoops": "^2.3",
    "packaged/dal": "^1.8",
    "packaged/dispatch": "~2.6",
    "packaged/glimpse": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*"
  },
  "autoload": {
    "psr-4": {
      "Project\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProjectTests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:cubex/framework.git"
    }
  ]
}
